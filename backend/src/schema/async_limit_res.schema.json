{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://marktverbund25.at/async_limit_res.schema.json",
    "title": "limit order final state response",
    "description": "format of response body lim. order expiration/fullfillment/cancelation",
    "type": "object",
    "properties": {
        "lim_id": {
            "type": "number",
            "exclusiveMinimum": 0
        },
        "status": {
            "type": "string",
            "enum": [
                "fulfilled",
                "expired",
                "canceled"
            ]
        }
    },
    "required": [
        "lim_id",
        "status"
    ]
}